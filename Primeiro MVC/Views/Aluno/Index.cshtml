@{
    ViewData["Title"] = "Listar Alunos";
}
<h1> Listar Alunos</h1>

<table>
    <thead>
        <tr>
            <th>id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Matricula</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var aluno in Model)
        {
            <tr>
                <td>@aluno.AlunoId</td>
                <td>@aluno.Nome</td>
                <td>@aluno.Email</td>
                <td>@aluno.RM</td>
                </tr>
                }
        </tbody>
    </table>
    <track />